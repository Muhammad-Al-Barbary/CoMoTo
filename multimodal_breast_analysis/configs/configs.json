{
    "device": 0,
    "seed": 0,
    "data": {
        "student_name": "penn_fudan",
        "teacher_name": "penn_fudan",
        "student_path": "PennFudanPed",
        "teacher_path": "PennFudanPed",
        "test_split": 0.2,
        "batch_size": 2,
        "shuffle": true
    },
    "transforms": {
      "size": [512,512]
    },
    "networks": { 
        "student": "faster_rcnn",
        "student_parameters": {
            "num_classes":2,
            "backbone": "resnet18",
            "anchors_sizes": [[32, 64, 128, 256, 512]],
            "anchors_aspect_ratios": [[0.5, 1.0, 2.0]],
            "align_size": 7,
            "align_sample_ratio": 2
        },
        "teacher": "faster_rcnn",
        "teacher_parameters": {
            "num_classes":2,
            "backbone": "resnet18",
            "anchors_sizes": [[32, 64, 128, 256, 512]],
            "anchors_aspect_ratios": [[0.5, 1.0, 2.0]],
            "align_size": 7,
            "align_sample_ratio": 2
        },
        "student_cp": "checkpoints/student.pt",
        "teacher_cp": "checkpoints/teacher.pt"
    },
    "train": {
        "warmup_epochs": 2,
        "epochs": 2,
        "distill_epoch": 0,
        "student_optimizer": "adam",
        "student_optimizer_parameters": {"lr": 0.01},
        "student_scheduler":"step",
        "student_scheduler_parameters":{"step_size":20, "gamma":0.5, "verbose":0},
        "teacher_optimizer": "adam",
        "teacher_optimizer_parameters": {"lr": 0.01},
        "teacher_scheduler":"step",
        "teacher_scheduler_parameters":{"step_size":20, "gamma":0.5, "verbose":0},
        "distill_coeff": 3,
        "temperature": 1,
        "alpha": 2
    }
}